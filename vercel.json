{
    "version": 2,
    "builds": [
      {
        "src": "flaskapp.py",
        "use": "@vercel/python@2.0.0",
        "config": {
          "maxLambdaSize": "50mb",
          "includeFiles": [
            "data/**"
          ]
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "flaskapp.py"
      }
    ],
    "env": {
      "PYTHONPATH": ".",
      "VERCEL_PYTHON_RUNTIME": "python3.9"
    }
}
  