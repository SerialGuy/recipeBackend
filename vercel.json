{
    "version": 2,
    "builds": [
      {
        "src": "flaskapp.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "50mb"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "flaskapp.py"
      }
    ],
    "env": {
      "PYTHONPATH": "."
    }
}
  