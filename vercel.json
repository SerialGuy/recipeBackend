{
    "version": 2,
    "builds": [
      {
        "src": "build.sh",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "."
        }
      },
      {
        "src": "flaskapp.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "flaskapp.py"
      }
    ],
    "env": {
      "VERCEL_DEBUG": "1"
    }
}
  